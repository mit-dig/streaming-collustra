{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "markdown": {
    "parser": "gfm",
    "tags": ["todo"]
  },
  "opts": {
    "private": true,
    "lenient": true,
    "recurse": true,
    "destination": "doc"
  }
}
